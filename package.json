{
  "name": "webpackEgghead",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "elint": "eslint . — ext .js — ext .ts — ext .tsx — cache || true",
    "start": "nodemon server/server.js --watch common --watch server --watch --content-base src/",
    "posttest": "npm run lint && nsp check",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.test.ts' ./src/",
    "typedoc": "typedoc",
    "tslint": "tslint"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "istanbul"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "block-no-empty": null,
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^7.0.39",
    "@types/react": "^15.0.38",
    "@types/react-dnd": "^2.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.46",
    "@types/redux-form": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "cls-hooked": "^4.2.0",
    "compression": "^1.0.3",
    "copy-webpack-plugin": "^4.0.1",
    "cors": "^2.5.2",
    "css-loader": "^0.28.4",
    "draft-js-dnd-plugin": "^2.0.0-beta1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "helmet": "^1.3.0",
    "html-webpack-plugin": "^2.28.0",
    "last-draft": "^3.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-connector-mongodb": "^1.18.1",
    "loopback-context": "^3.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.10",
    "react": "15.5.0",
    "react-bootstrap-dropdown-menu": "^1.1.11",
    "react-dropzone": "^3.13.3",
    "react-modal": "^1.7.3",
    "react-notifications": "^1.4.3",
    "react-places-autocomplete": "^5.3.1",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.5",
    "react-svg": "^2.1.21",
    "recompose": "^0.24.0",
    "redux": "^3.6.0",
    "redux-form": "^7.0.1",
    "redux-saga": "^0.14.6",
    "sass-loader": "^6.0.6",
    "serve-favicon": "^2.0.1",
